	publisher = {ACM},
	author = {Filos-Ratsikas, Aris and Hansen, Kristoffer Arnsfelt and Høgh, Kasper and Hollender, Alexandros},
	month = jun,
	year = {2024},
	keywords = {notion},
	pages = {1204--1215},
	file = {Filos-Ratsikas et al. - 2024 - PPAD-Membership for Problems with Exact Rational S.pdf:C\:\\Users\\91124\\Zotero\\storage\\DT6SUBAA\\Filos-Ratsikas et al. - 2024 - PPAD-Membership for Problems with Exact Rational S.pdf:application/pdf},
