	file = {PDF:C\:\\Users\\91124\\Zotero\\storage\\MELKNKW8\\Sherali and Adams - 1994 - A hierarchy of relaxations and convex hull characterizations for mixed-integer zero—one programming.pdf:application/pdf},
}

